{
  "Silk": {
    "SelfHosted": false,
    "Persistence": {
      "Host": "localhost",
      "Port": "5432",
      "Database": "<database-name>",
      "Username": "postgres",
      "Password": "<database-user-password>"
    },
    "Discord": {
      "Shards": 1,
      "ClientId": "<discord-client-id>", /* Set when using Dashboard, otherwise can be left as is */
      "ClientSecret": "<discord-client-secret>", /* Set when using Dashboard, otherwise can be left as is */
      "BotToken": "<discord-bot-token>" 
    },
    /* Set if want or have an E621 account, otherwise can be left as is */
    "E621": {
      "ApiKey": "<api-key>",
      "Username": "<username>"
    }
  }
}